node{
    stage('SCM Checkout'){
        git credentialsId: 'gitid', url: 'http://github.com/java/javahometech/my-app'
    }
    stage('MVN Package'){
        
        def mvnHome = tool name: 'localMaven', type: 'maven'
        def mvnCMD = "${mvnHome}/bin/mvn"
        sh "${mvnCMD} Clean Package"
    }
}
